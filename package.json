{
  "name": "api-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prettier-format": "prettier --config .prettierrc './**/*.{js,jsx,ts,tsx,css,md,json}' --write",
    "prepare": "husky install",
    "create-hook": "husky add .husky/pre-commit \"npm run prettier-format && npm --prefix ./client run lint\""
  },
  "dependencies": {},
  "devDependencies": {
    "husky": "^8.0.3",
    "prettier": "^3.0.2"
  }
}
